{% extends 'base.html.twig' %}

{% block body %}
    <h1>Create new user</h1>

    {{ form_start(form) }}
    {{ form_row(form.login) }}
    {{ form_row(form.password) }}
    {{ form_row(form.name) }}
    {{ form_row(form.first_name) }}
    {{ form_row(form.Birth_day) }}
    <button type="submit" class="btn btn-primary">Save</button>
    {{ form_end(form) }}
{% endblock %}